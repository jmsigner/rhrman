issn = {1932-6203},
journal = {PloS one},
keywords = {Animals,Appetitive Behavior,Appetitive Behavior: physiology,Behavior, Animal,Ecosystem,Feeding Behavior,Movement,Papio ursinus,Papio ursinus: physiology,Walking,Walking: psychology},
month = jan,
