issn = {1461-0248},
journal = {Ecology letters},
keywords = {Animal Migration,Animal Migration: physiology,Animals,Ecology,Ecology: trends,Environment,Models, Biological,Movement,Movement: physiology,Population Dynamics},
month = dec,
number = {12},
pages = {1338--50},
