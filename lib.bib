issn = {1872-678X},
journal = {Journal of neuroscience methods},
keywords = {Animals,Brain Chemistry,Cerebellum,Cerebellum: chemistry,Linear Models,Male,Neurosciences,Neurosciences: methods,Rats,Rats, Sprague-Dawley,Regression Analysis},
month = oct,
number = {1},
