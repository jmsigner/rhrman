journal = {Primates; journal of primatology},
keywords = {Animals,China,Colobinae,Colobinae: physiology,Data Interpretation, Statistical,Demography,Geographic Information Systems,Homing Behavior},
month = jan,
number = {1},
pages = {81--4},
pmid = {19020948},
title = {{Choice of analytical method can have dramatic effects on primate home range estimates.}},
