journal = {Philosophical transactions of the Royal Society of London. Series B, Biological sciences},
keywords = {Alberta,Animal Migration,Animals,Animals, Wild,Data Interpretation, Statistical,Female,Geographic Information Systems,Seasons,Telemetry},
month = jul,
