journal = {The American naturalist},
keywords = {Animals,Ants,Appetitive Behavior,Models, Biological,Movement},
month = oct,
number = {4},
pages = {506--14},
pmid = {19737110},
title = {{Fundamental insights into the random movement of animals from a single distance-related statistic.}},
