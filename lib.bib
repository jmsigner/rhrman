journal = {PloS one},
keywords = {Animals,Artiodactyla,Artiodactyla: genetics,Artiodactyla: physiology,Deer,Deer: genetics,Genetic Variation,Humans},
month = jan,
number = {5},
pages = {e20144},
pmid = {21625459},
title = {{Effect of anthropogenic landscape features on population genetic differentiation of Przewalski's gazelle: main role of human settlement.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3098875\&tool=pmcentrez\&rendertype=abstract},
